@model Tema2_NoLogin.Models.Appointment

@{
    ViewData["Title"] = "Download";
}

<h1>Download</h1>

<h4>Appointment</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post" asp-action="Download">
            <div class="form-group">
                <label for="first_Date">First Date</label>
                <input type="text" name="first_Date" />
            </div>
            <div class="form-group">
                <label for="last_Date">Last Date</label>
                <input type="text" name="last_Date" />
            </div>

            <div class="form-group">
                <input type="text" name="typeDoc" />
            </div>
            <div class="form-group">
                <input type="submit" value="Download" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>
